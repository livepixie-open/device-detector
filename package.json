{
  "name": "@livepixie-open/device-detector",
  "version": "1.0.0",
  "description": "A Server-Side Rendering (SSR) library to detect Material UI breakpoints from user agent.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest --no-watchman --coverage",
    "lint": "eslint . --ext .ts --fix",
    "format": "prettier --write .",
    "clean": "rm -rf dist",
    "prepare": "pnpm clean && pnpm build"
  },
  "keywords": [
    "device-detection",
    "user-agent",
    "ua-parser",
    "SSR",
    "Material-UI",
    "breakpoints",
    "nextjs",
    "server-side-rendering"
  ],
  "author": "Live Pixie Open Source",
  "license": "MIT",
  "packageManager": "pnpm@10.4.1",
  "dependencies": {
    "ua-parser-js": "2.0.2"
  },
  "devDependencies": {
    "@eslint/js": "9.21.0",
    "@types/jest": "29.5.14",
    "@types/node": "22.13.5",
    "@typescript-eslint/eslint-plugin": "8.24.1",
    "@typescript-eslint/parser": "8.24.1",
    "eslint": "9.21.0",
    "eslint-config-prettier": "10.0.1",
    "eslint-plugin-jest": "28.11.0",
    "eslint-plugin-jest-dom": "5.5.0",
    "jest": "29.7.0",
    "prettier": "3.5.2",
    "ts-jest": "29.2.5",
    "typescript": "5.7.3"
  },

  "publishConfig": {
    "access": "public"
  }
}
